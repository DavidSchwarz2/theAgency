{
  "name": "frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "root": "frontend",
  "sourceRoot": "frontend/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run dev", "cwd": "frontend" }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run build", "cwd": "frontend" }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run lint", "cwd": "frontend" }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": { "command": "npx tsc --noEmit", "cwd": "frontend" }
    }
  }
}
